amanda_32: ricardo_zlib32.a dll_32.o zlib_32.o longfile_32.o md5_32.o folder_32.o amandaricardo_koci_dprintf_32.o \
	sha512_k_32.o ricardo_64
	
	@./amandaricardo_mvbuild26_32.exe ./amanda_file_counter_32.ini versio_n.ama.rc version_ricardo_mislaine_final_2_.rc aline_definitive_header_2_.h "32 bits" rspgzip200_32.dll increase

	@windres.exe -o ver32.o -O coff version_ricardo_mislaine_final_2_.rc
	
	gcc -O3 -mdll -DNPRINTF_NO  dprintf.c def_k.def dll_32.o amandaricardo_koci_dprintf_32.o \
	sha512_k_32.o \
	\
	zlib_32.o \
	longfile_32.o \
	md5_32.o \
	\
	ver32.o \
	folder.c -o rspgzip200_32.dll -Wl,-enable-stdcall-fixup ricardo_zlib32.a 
	@cp rspgzip200_32.dll ../rspgzip200_32.dll
	@cp rspgzip200_32.dll  'C:\Amanda Ava Pereira\nosso_trabalho\amanda_projects\Lurdes_tar_gzip_parolin_k\files_k\tar_sample_ar\tar_sample_ar\bin\Release\'
amandaricardo_koci_dprintf_32.o: amandaricardo_koci_dprintf.c
	gcc -O3 -c -o amandaricardo_koci_dprintf_32.o amandaricardo_koci_dprintf.c

dll_32.o: dll.c
	gcc -O3 -c -o dll_32.o dll.c
zlib_32.o: zlib.c
	gcc -O3 -c -o zlib_32.o zlib.c
longfile_32.o: longfile.c
	gcc -O3 -c -o longfile_32.o longfile.c
md5_32.o: md5.c
	gcc -O3 -c -o md5_32.o md5.c
folder_32.o: folder.c
	gcc -O3 -c -o folder_32.o folder.c
sha512_k_32.o: sha512_k.c
	gcc -O3 -c -o sha512_k_32.o sha512_k.c

id:

ricardo_zlib32.a:
#touch ./zlib_src/adler32.c
	@make amanda_win32 -C ./zlib_src/
	
ricardo_64: amanda_zlib64.a dll_64.o zlib_64.o longfile_64.o md5_64.o folder_64.o amandaricardo_koci_dprintf_64.o \
	sha512_k_64.o
	
	@./amandaricardo_mvbuild26_32.exe ./amanda_file_counter_32.ini versio_n.ama.rc version_ricardo_mislaine_final_2_.rc aline_definitive_header_2_.h "64 bits" rspgzip200_64.dll increase

	@x86_64-w64-mingw32-windres.exe -o ver64.o -O coff version_ricardo_mislaine_final_2_.rc
	
	x86_64-w64-mingw32-gcc.exe -DNPRINTF_NO -O3 -mdll def_k.def dprintf.c \
	\
	dll_64.o zlib_64.o longfile_64.o md5_64.o folder_64.o amandaricardo_koci_dprintf_64.o \
	sha512_k_64.o \
	\
	ver64.o \
	\
	-o rspgzip200_64.dll -Wl,-enable-stdcall-fixup amanda_zlib64.a
	
	cp rspgzip200_64.dll ../rspgzip200_64.dll
	cp rspgzip200_64.dll 'C:\Amanda Ava Pereira\nosso_trabalho\amanda_projects\Lurdes_tar_gzip_parolin_k\files_k\tar_sample_ar_x64\tar_sample_ar\bin\Release'
amanda_zlib64.a:
	@make ricardo_win64 -C ./zlib_src/

amandaricardo_koci_dprintf_64.o: amandaricardo_koci_dprintf.c
	x86_64-w64-mingw32-gcc -O3 -c -o amandaricardo_koci_dprintf_64.o amandaricardo_koci_dprintf.c	

dll_64.o: dll.c
	x86_64-w64-mingw32-gcc -O3 -c -o dll_64.o dll.c
zlib_64.o: zlib.c
	x86_64-w64-mingw32-gcc -O3 -c -o zlib_64.o zlib.c
longfile_64.o: longfile.c
	x86_64-w64-mingw32-gcc -O3 -c -o longfile_64.o longfile.c
md5_64.o: md5.c
	x86_64-w64-mingw32-gcc -O3 -c -o md5_64.o md5.c
folder_64.o: folder.c
	x86_64-w64-mingw32-gcc -O3 -c -o folder_64.o folder.c
sha512_k_64.o: sha512_k.c
	x86_64-w64-mingw32-gcc -O3 -c -o sha512_k_64.o sha512_k.c