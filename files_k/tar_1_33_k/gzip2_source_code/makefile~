AMANDA_FLAGS__=-O3 -Wall -Wfatal-errors -Werror
FELINE_FLAGS__=-Wno-unused-function
JULIET_FLAGS  =

AMANDA_SIZE   =                                    62

amanda_32: pedro_32.o dll_32.o folder_i_32.o       \
	longfile_32.o lzop2_multi_32.o             \
	sha512_k_32.o                              \
	stdio_v2_32.o zlib_32.o miniz_32.o

	@i686-w64-mingw32-gcc.exe --version        \
	>feline_gcc_version_x86.ric

	@./file_gcc_version.exe read_i             \
	feline_gcc_version_x86.ric                 \
	pedro_32.o dll_32.o folder_i_32.o          \
	longfile_32.o lzop2_multi_32.o             \
	sha512_k_32.o                              \
	stdio_v2_32.o zlib_32.o miniz_32.o

	@./amandaricardo_mvbuild26_32.exe          \
	./amanda_file_counter_32.ini               \
	versio_n.ama.rc                            \
	version_ricardo_mislaine_final_2_.rc       \
	aline_definitive_header_2_.h "32 bits"     \
	rsp_gzip2_32.DLL increase

	@i686-w64-mingw32-windres.exe -o ver32.o   \
	-O coff                                    \
	version_ricardo_mislaine_final_2_.rc

	@i686-w64-mingw32-gcc.exe -mdll            \
	def_k.def                                  \
	pedro_32.o dll_32.o                        \
	folder_i_32.o longfile_32.o                \
	lzop2_multi_32.o                           \
	sha512_k_32.o                              \
	stdio_v2_32.o zlib_32.o miniz_32.o         \
	ver32.o                                    \
	-o rsp_gzip2_32.DLL                        \
	-Wl,-enable-stdcall-fixup

	@cp rsp_gzip2_32.DLL ../

	@cp rsp_gzip2_32.DLL                      'C:/Amanda Ava Pereira/nosso_trabalho/amanda_projects/Lurdes_tar_gzip_parolin_k/files_k/tar_sample_ar/tar_sample_ar/bin/Release/'

	@./espaco_arp.exe $(AMANDA_SIZE)

miniz_32.o: miniz.c
	i686-w64-mingw32-gcc.exe -c -o             \
	miniz_32.o                                 \
	miniz.c                                    \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)          \
	$(FELINE_FLAGS__)

	@i686-w64-mingw32-gcc.exe --version        \
	>feline_gcc_version_x86.ric

	@./file_gcc_version.exe write_i            \
	feline_gcc_version_x86.ric                 \
	miniz_32.o

	@./espaco_arp.exe $(AMANDA_SIZE)

lzop2_multi_32.o: stdio_v2.c stdio_v2.h            \
	lzop2_thread.c gzip2_include.h lzop2_multi.c
	i686-w64-mingw32-gcc.exe -c -o             \
	lzop2_multi_32.o lzop2_multi.c             \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)          \
	$(FELINE_FLAGS__)

	@i686-w64-mingw32-gcc.exe --version        \
	>feline_gcc_version_x86.ric

	@./file_gcc_version.exe write_i            \
	feline_gcc_version_x86.ric                 \
	lzop2_multi_32.o

	@./espaco_arp.exe $(AMANDA_SIZE)

