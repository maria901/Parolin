#2021 amanda & ricardo
AMANDA_FLAGS__=-O3 -Wall -Wfatal-errors -Werror
FELINE_FLAGS__=-Wno-unused-function -I ./
JULIET_FLAGS  = 
amanda_32: amandaricardo_koci_dprintf_32arp.o    \
	lz4_32arp.o lz4_frame_arp_32arp.o            \
	lz4frame_32arp.o lz4hc_32arp.o               \
	xxhash_32arp.o ricardo_64

	i686-w64-mingw32-gcc.exe -shared arp.def -o  \
	rsp_lz4_arp_32.DLL                           \
	amandaricardo_koci_dprintf_32arp.o           \
	lz4_32arp.o lz4_frame_arp_32arp.o            \
	lz4frame_32arp.o lz4hc_32arp.o               \
	xxhash_32arp.o                               \
	-Wl,-enable-stdcall-fixup -static-libgcc     \
	-O3
	
	@cp rsp_lz4_arp_32.DLL ../../../
	@cp rsp_lz4_arp_32.DLL  'C:\Amanda Ava Pereira\nosso_trabalho\amanda_projects\Lurdes_tar_gzip_parolin_k\files_k\tar_sample_ar\tar_sample_ar\bin\Release\'
lz4_frame_arp_32arp.o : helper_utilities_by_amanda_and_ricardo.c \
	lz4_frame_arp.c
	i686-w64-mingw32-gcc.exe -c -o               \
	lz4_frame_arp_32arp.o lz4_frame_arp.c        \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
lz4_32arp.o : lz4.c
	i686-w64-mingw32-gcc.exe -c -o lz4_32arp.o   \
	lz4.c                                        \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
lz4frame_32arp.o : lz4frame.c
	i686-w64-mingw32-gcc.exe -c -o               \
	lz4frame_32arp.o lz4frame.c                  \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
lz4hc_32arp.o : lz4hc.c
	i686-w64-mingw32-gcc.exe -c -o lz4hc_32arp.o \
	lz4hc.c                                      \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
xxhash_32arp.o : xxhash.c
	i686-w64-mingw32-gcc.exe -c -o               \
	xxhash_32arp.o xxhash.c                      \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
amandaricardo_koci_dprintf_32arp.o : amandaricardo_koci_dprintf.c
	i686-w64-mingw32-gcc.exe -c -o               \
	amandaricardo_koci_dprintf_32arp.o           \
	amandaricardo_koci_dprintf.c                 \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83

#################################64 bits

ricardo_64: amandaricardo_koci_dprintf_64arp.o   \
	lz4_64arp.o lz4_frame_arp_64arp.o            \
	lz4frame_64arp.o lz4hc_64arp.o               \
	xxhash_64arp.o

	x86_64-w64-mingw32-gcc -shared arp.def -o    \
	rsp_lz4_arp_64.DLL -O3                       \
	amandaricardo_koci_dprintf_64arp.o           \
	lz4_64arp.o lz4_frame_arp_64arp.o            \
	lz4frame_64arp.o lz4hc_64arp.o               \
	xxhash_64arp.o                               \
	-Wl,-enable-stdcall-fixup -static-libgcc
	@cp rsp_lz4_arp_64.DLL ../../../
	@cp rsp_lz4_arp_64.DLL  'C:\Amanda Ava Pereira\nosso_trabalho\amanda_projects\Lurdes_tar_gzip_parolin_k\files_k\tar_sample_ar_x64\tar_sample_ar\bin\Release'
	
lz4_frame_arp_64arp.o : helper_utilities_by_amanda_and_ricardo.c \
	lz4_frame_arp.c
	x86_64-w64-mingw32-gcc.exe -c -o             \
	lz4_frame_arp_64arp.o lz4_frame_arp.c        \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
lz4_64arp.o : lz4.c
	x86_64-w64-mingw32-gcc.exe -c -o lz4_64arp.o \
	lz4.c                                        \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
