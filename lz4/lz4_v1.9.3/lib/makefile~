	x86_64-w64-mingw32-gcc.exe -c -o             \
	lz4frame_64arp.o lz4frame.c                  \
	$(AMANDA_FLAGS__) $(JULIET_FLAGS)            \
	$(FELINE_FLAGS__)
	@./espaco_arp.exe 83
	
