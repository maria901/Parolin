#2022 ric & dl 
DL____FLAGS__=-O3 -Wall -Wfatal-errors -Werror -Wno-sign-compare -Wno-restrict -Wextra -Wno-unused-function -I .
DL____FLAGS__2=-O1 -Wfatal-errors -Werror -Wno-restrict -Wno-unused-function -I .

pedro_dl_.o: pedro.c
	x86_64-w64-mingw32-gcc.exe $(DL____FLAGS__) -c -o pedro_dl_.o pedro.c
dl_504: pedro_dl_.o
	x86_64-w64-mingw32-gcc.exe $(DL____FLAGS__) -Wl,-subsystem,windows new_lzss_dl.c -o pois_e_dl.exe pedro_dl_.o
	./pois_e_dl.exe
	-./ari.exe < make_exe_ric_dl__lsss > make_exe_ric_dl__lzss_exxe
