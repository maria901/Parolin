	src/help_64aakp_rspk.o src/lzop_64aakp_rspk.o                 src/mblock_64aakp_rspk.o \
	src/p_lzo_64aakp_rspk.o src/s_curses_64aakp_rspk.o          src/s_djgpp2_64aakp_rspk.o \
