AMANDA_FLAGS__=-O3 -I ./include
amanda_32: src/c_ansic_32aakp_rspk.o src/c_ansim_32aakp_rspk.o    src/c_init_32aakp_rspk.o \
	       src/c_none_32aakp_rspk.o src/c_screen_32aakp_rspk.o  src/compress_32aakp_rspk.o \
	       src/djgpp2_32aakp_rspk.o src/filter_32aakp_rspk.o      src/frames_32aakp_rspk.o \
	       src/help_32aakp_rspk.o src/lzop_32aakp_rspk.o          src/mblock_32aakp_rspk.o \
	       src/p_lzo_32aakp_rspk.o src/s_curses_32aakp_rspk.o   src/s_djgpp2_32aakp_rspk.o \
	       src/s_object_32aakp_rspk.o src/s_vcsa_32aakp_rspk.o      src/util_32aakp_rspk.o \
	       amandaricardo_koci_dprintf_32aakp_rspk.o ricardo_64

	gcc -O3 -mdll -o BE_lzop_arp_32.DLL                                                    \
	src/c_ansic_32aakp_rspk.o src/c_ansim_32aakp_rspk.o           src/c_init_32aakp_rspk.o \
	src/c_none_32aakp_rspk.o src/c_screen_32aakp_rspk.o         src/compress_32aakp_rspk.o \
	src/djgpp2_32aakp_rspk.o src/filter_32aakp_rspk.o             src/frames_32aakp_rspk.o \
	src/help_32aakp_rspk.o src/lzop_32aakp_rspk.o                 src/mblock_32aakp_rspk.o \
	src/p_lzo_32aakp_rspk.o src/s_curses_32aakp_rspk.o          src/s_djgpp2_32aakp_rspk.o \
	src/s_object_32aakp_rspk.o src/s_vcsa_32aakp_rspk.o             src/util_32aakp_rspk.o \
	src/amandaricardo_koci_dprintf_32aakp_rspk.o                      liblzo_32aakp_rspk.a \
	-Wl,-enable-stdcall-fixup arp.def
	@cp BE_lzop_arp_32.DLL ../../
	@cp BE_lzop_arp_32.DLL  'C:\Amanda Ava Pereira\nosso_trabalho\amanda_projects\Lurdes_tar_gzip_parolin_k\files_k\tar_sample_ar\tar_sample_ar\bin\Release\'
	
